{// 引入标签库 }
<tagLib name="html" />
<include file="Public:header" />
<load href='__TMPL__Common/style/jquery.autocomplete.css' />
<load href='__TMPL__Common/js/jquery.autocomplete.min.js' />
<div class="main">
<div class="main_title">设定提现密码</div>
<div class="blank5"></div>

<form name="edit" action="__APP__" method="post" enctype="multipart/form-data">
<div class="blank5"></div>
<table class="form conf_tab" cellpadding=0 cellspacing=0 rel="1">
	<tr>
		<td colspan=2 class="topTd"></td>
	</tr>
	<tr>
		<td class="item_title">原始密码:</td>
		<td class="item_input"><input type="password" class="textbox" name="old_password" size="17" /></td>
	</tr>
	<tr>
		<td class="item_title">新密码:</td>
		<td class="item_input"><input type="password" class="textbox" name="new_password" size="17" /></td>
	</tr>
	<tr>
		<td class="item_title">确认密码:</td>
		<td class="item_input"><input type="password" class="textbox" name="confirm_password" size="17" /></td>
	</tr>
	<tr>
		<td colspan=2 class="bottomTd"></td>
	</tr>
</table>
<div class="blank5"></div>
<table class="form" cellpadding=0 cellspacing=0>
	<tr>
		<td colspan=2 class="topTd"></td>
	</tr>
	<tr>
		<td class="item_title">操作密码:</td>
		<td class="item_input">
			<input type="password" class="textbox" name="operatepwd" value=""/>
		</td>
	</tr>
	<tr>
		<td class="item_title"></td>
		<td class="item_input">
		<!--隐藏元素-->
		<input type="hidden" name="{:conf("VAR_MODULE")}" value="Carry" />
		<input type="hidden" name="{:conf("VAR_ACTION")}" value="update_passset" />
		<!--隐藏元素-->
		<input type="submit" class="button" value="确定" />
		<input type="reset" class="button" value="{%RESET}" />
		</td>
	</tr>
	<tr>
		<td colspan=2 class="bottomTd"></td>
	</tr>
</table> 	 
</form>
</div>
<include file="Public:footer" />