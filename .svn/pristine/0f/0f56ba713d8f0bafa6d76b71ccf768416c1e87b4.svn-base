{// 引入标签库 }
<tagLib name="html" />
<include file="Public:header" />
<load href='/app/Tpl/new/js/script.js' />
<load href='__TMPL__Common/js/jquery.bgiframe.js' />
<div class="main main-size16">
<div class="main_title">{$main_title}</div>
<div class="blank5"></div>
<div class="button_row">
	<input type="button" class="button" value="新增权限分配" onclick="add();" />
	<input type="button" class="button" value="编辑权限分配" onclick="_edit()" />
</div>
<div class="blank5"></div>
<html:list 
	id="dataTable" 
	style="dataTable dataTableSelect"
	name="conf" 
	datasource="list" 
	show="id|checkbox:选择|50px
		,adm_name:用户ID
		,adm_id|get_admin_real_name:用户名
		,create_time|to_date:创建时间
		,update_time|to_date:更新时间
		,is_effect|get_is_effect:是否有效|80px
		,empty_password:是否空密码
		,modules:操作模块"
	/> 

<div class="blank5"></div>
<div class="page">{$page}</div>
</div>
<include file="Public:footer" />