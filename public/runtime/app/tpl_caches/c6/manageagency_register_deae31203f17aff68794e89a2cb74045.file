					else
					{	
						is_lock_send_vys = false;
						formError($("#signup-email-address"),result.info);
						return false;
					}
				}
			});	
		}); //发送邮箱验证码
		
	//手机验证
	$("#signup-mobile-submit").click(function(){
		var name;
		name=$.trim($("#signup-user-form #signup-password").val())
		var reg=/^[a-zA-z0-9]{6,}$/;  
		var regs=/^(?![0-9]+$)(?![a-zA-Z]+$)[0-9A-Za-z]{6,16}$/;
