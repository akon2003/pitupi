function checkRegsiter(obj){
	
	if(!$.minLength(obj.find("#signup-username").val(),3,true))
	{
		obj.find("#signup-username").focus();
		//console.log(obj.find("#signup-username").val());
		$.showErr("机构名称格式错误，请重新输入");		
		return false;
	}
	if(!$.maxLength(obj.find("#signup-username").val(),16,true))
	{
		obj.find("#signup-username").focus();
		$.showErr("机构名称格式错误，请重新输入");			
		return false;
	}
	
		if(!$.checkMobilePhone($("#settings-mobile").val()))
		{
			$("#settings-mobile").focus();			
			$.showErr("手机号码格式错误，请重新输入");	
			return false;
		}	
		if($.trim($("#settings-mobile").val()).length == 0)
		{
			$("#settings-mobile").focus();
			$.showErr("手机号码不能为空");
			return false;
		}
	
		if($.trim($("#signup-email-address").val()).length == 0 )
		{
			$("#signup-email-address").focus();
			$.showErr("Email不能为空");
			return false;
		}
		if(!$.checkEmail($("#signup-email-address").val()))
		{
			$("#signup-email-address").focus();			
			$.showErr("Email格式错误，请重新输入");
			return false;
		}
	
	
	if(!$.minLength(obj.find("#signup-password").val(),4,false))
	{
		obj.find("#signup-password").focus();
		$.showErr("密码格式错误，请重新输入");	
		return false;
	}
	if(obj.find("#signup-password-confirm").val()!==""){
		if(obj.find("#signup-password-confirm").val() != obj.find("#signup-password").val())
		{
			obj.find("#signup-password-confirm").focus();
			$.showErr("密码确认失败");			
			return false;
		}
	}
	else{
		obj.find("#signup-password-confirm").focus();
		$.showErr("请输入确认密码");			
		return false;
	}//密码
	
	if(obj.find("#J_agreement:checked").length == 0){
		$.showErr("请先同意  会员服务协议");
		return false;
	}
	$(obj).find("#JverifyImg").remove();
	var html = obj.clone();
	$("#j_submit_form").html(html);
	$("#j_submit_form input").each(function(){
		$(this).val(obj.find("input[name='"+$(this).attr("name")+"']").val());
	});
	$("#j_submit_form textarea").each(function(){
		$(this).val(obj.find("textarea[name='"+$(this).attr("name")+"']").val());
	});
	$("#j_submit_form select").each(function(){
		$(this).val(obj.find("select[name='"+$(this).attr("name")+"']").val());
	});
	
	$("#j_submit_form").submit();
}
</script>
	
